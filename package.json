    "aws-sdk": "^2.802.0",
    "bcrypt": "^5.0.0",
    "cloudinary": "^1.23.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.12",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "pg": "^8.5.1",
    "postgrator-cli": "^3.2.0",
    "uuid": "^8.3.1",
    "xss": "^1.0.8"
  },
  "devDependencies": {
